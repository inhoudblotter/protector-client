{
  "name": "protector-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx ./src/server/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run build:client && npm run build:server && npm run copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr ./src/client/app/entry.server.tsx && vite build --ssr ./src/server/index.ts",
    "start": "npm run build && cross-env NODE_ENV=production node ./dist/index.js",
    "copy-files": "copyfiles -u 2 \"dist/client/assets/*\" public && rimraf dist/client/assets && copyfiles -u 2 \"dist/client/**/*\" dist && rimraf dist/client"
  },
  "dependencies": {
    "@types/yandex-maps": "^2.1.32",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "preact": "^10.16.0",
    "preact-render-to-string": "^6.2.2",
    "preact-router": "^4.1.2",
    "serve-static": "^1.15.0",
    "tsx": "^3.14.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/compression": "^1.7.4",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-preact": "^1.3.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
